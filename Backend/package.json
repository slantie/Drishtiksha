{
    "name": "backend",
    "version": "1.0.0",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles",
        "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=unit --detectOpenHandles --passWithNoTests",
        "test:integration": "node scripts/run-integration-tests.js",
        "test:real-analysis": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=real-analysis --detectOpenHandles --testTimeout=1800000",
        "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=e2e --detectOpenHandles --testTimeout=900000",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --detectOpenHandles",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles",
        "start": "node server.js",
        "dev": "nodemon server.js",
        "worker": "node src/workers/media.worker.js",
        "assets": "nodemon assets.server.js",
        "start:full": "concurrently \"npm run start\" \"npm run worker\" \"npm run studio\" --names \"SERVER,WORKER,STUDIO\" --prefix-colors \"cyan,magenta,green\"",
        "dev:full": "concurrently \"npm run dev\" \"npm run worker\" \"npm run db:studio\" \"npm run assets\" --names \"SERVER,WORKER,STUDIO,STORAGE\" --prefix-colors \"cyan,magenta,green,white\"",
        "studio": "npx prisma studio --hostname 0.0.0.0 --port 5555",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:studio": "prisma studio"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "concurrently": "^9.1.2",
        "express": "^4.21.2",
        "jest": "^29.7.0",
        "nock": "^14.0.10",
        "nodemon": "^3.1.10",
        "prisma": "^6.14.0",
        "supertest": "^7.0.0"
    },
    "dependencies": {
        "@prisma/client": "^6.14.0",
        "axios": "^1.9.0",
        "bcryptjs": "^3.0.2",
        "bullmq": "^5.58.0",
        "cloudinary": "^2.7.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.5.0",
        "express-rate-limit": "^7.5.1",
        "express-validator": "^7.2.1",
        "form-data": "^4.0.4",
        "helmet": "^8.1.0",
        "ioredis": "^5.7.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "multer": "^2.0.2",
        "path-to-regexp": "^6.3.0",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "winston": "^3.17.0",
        "zod": "^4.0.17"
    }
}
